<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bermarte.games" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>games</name>
    <description>
        A sample Apache Cordova application.
    </description>
    <author email="" href="">
        bermarte
    </author>
    <content src="index.html" />
    <icon src="res/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreen" value="/img/browser/splashscreen.png" />
    <preference name="SplashScreenBackgroundColor" value="#4d8f93" />
    <preference name="SplashScreenWidth" value="600" />
    <preference name="SplashScreenHeight" value="300" />
    <preference name="SplashMaintainAspectRatio" value="true" />
</widget>
